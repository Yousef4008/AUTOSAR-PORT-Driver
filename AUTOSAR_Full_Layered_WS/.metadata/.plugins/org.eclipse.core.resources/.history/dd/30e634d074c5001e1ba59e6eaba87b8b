#ifndef MEMMAP_H_
#define MEMMAP_H_


 /* Id for the company in the AUTOSAR
 * for example ID = 23 :) */
#define <MSN>_VENDOR_ID                 (23U)

/* <MSN> Module Id */
#define <MSN>_MODULE_ID                 (--U)

/* Instance Id */
#define <MSN>_INSTANCE_ID               (--U)

/*
 * <MSN> Software Module Version 1.0.0
 */
#define <MSN>_SW_MAJOR_VERSION          (1U)
#define <MSN>_SW_MINOR_VERSION          (0U)
#define <MSN>_SW_PATCH_VERSION          (0U)

/*
 * AUTOSAR Version 4.0.3
 */
#define <MSN>_AR_RELEASE_MAJOR_VERSION          (4U)
#define <MSN>_AR_RELEASE_MINOR_VERSION          (0U)
#define <MSN>_AR_RELEASE_PATCH_VERSION          (3U)

#endif /* MEMMAP_H_ */
